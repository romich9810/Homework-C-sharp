using System.Linq;
using System.Security.Cryptography.X509Certificates;
using System.Text;
using System.Threading.Tasks;

namespace CSLight
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int firstMemberOfRow = 5;
            int memberOfRow;
            int stepOfRow = 7;
            int lastMemberOfRow = 96;

            for (int i = 0;; i++)
            {
                memberOfRow = stepOfRow * i + firstMemberOfRow;
                
                if (memberOfRow > lastMemberOfRow)
                {
                    break;
                }

                Console.WriteLine(memberOfRow);
            }
        }
    }
